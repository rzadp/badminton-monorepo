{
  "plantuml.diagramsRoot": "thesis/chapters",
  "plantuml.exportOutDir": "thesis/chapters",
  "plantuml.exportSubFolder": false,
  "plantuml.exportFormat": "png",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "main.aux"
      ]
    },
    {
      "name": "mermaid",
      "command": "zsh",
      "args": [
        "./scripts/mermaid.sh"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "mythesis",
      "tools": [
        "mermaid",
        "bibtex",
        "pdflatex"
      ]
    },
  ],
}
